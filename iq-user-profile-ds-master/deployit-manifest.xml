<?xml version="1.0" encoding="UTF-8"?>
<udm.DeploymentPackage version="${BUILD_NUMBER}" application="${XLD_APPLICATION_FOLDER}/${REPO_NAME}${BRANCH_NAME_PLACEHOLDER}">
  <application />
  <orchestrator />
  <deployables>
    <k8s.ResourcesFile name="resource-${REPO_NAME}" file="resources.yaml">
      <tags />
      <scanPlaceholders>true</scanPlaceholders>
      <preScannedPlaceholders>true</preScannedPlaceholders>
      <placeholders />
    </k8s.ResourcesFile>
  </deployables>
  <applicationDependencies />
  <dependencyResolution>LATEST</dependencyResolution>
  <undeployDependencies>false</undeployDependencies>
</udm.DeploymentPackage>
